#include<bits/stdc++.h>
using namespace std;

void spiral_print(int arr[][4], int n, int m) {
    // taking four variables
    int startRow = 0;
    int endRow = n-1;
    int startColumn = 0;
    int endColumn = m-1;
    
    while(startRow <= endRow && startColumn <= endColumn) {
        // printing startRow
        for(int col = startColumn; col <= endColumn; col++) {
            cout << arr[startRow][col] << " ";
        }
        // printing endColumn
        for(int row = startRow+1; row <= endRow; row++) {
            if(startRow == endRow) {
                break;
            }
            cout << arr[row][endColumn] << " ";
        }
        // printing endRow
        for(int col = endColumn-1; col >= startColumn; col--) {
            cout << arr[endRow][col] << " ";
        }
        // printing startColumn
        for(int row = endRow-1; row >= startRow+1; row--) {
            if(startColumn == endColumn) {
                break;
            }
            cout << arr[row][startColumn] << " ";
        }
        // updating values
        startRow++;
        endRow--;
        startColumn++;
        endColumn--;
    }
}

int main() {
    int arr[][4] = { {1,2,3,4},
                   {12, 13,14,5},
                   {11,16,15,6},
                   {10,9,8,7} };
    spiral_print(arr, 4, 4);
}
